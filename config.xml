<?xml version='1.0' encoding='utf-8'?>
<widget id="br.com.misvivoapp.misvivoapp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>misvivoapp</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <vs:features>
        <vs:feature>onesignal-cordova-plugin</vs:feature>
    </vs:features>
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <gap:plugin name="pushbots-cordova-plugin" source="npm" spec="^1.4.0" />
    <preference name="phonegap-version" value="cli-6.3.0" />
    <preference name="android-build-tool" value="gradle" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="onesignal-cordova-plugin" spec="~2.0.11" />
</widget>
